{
  "name": "fastify-mongoose-starter",
  "version": "1.0.0",
  "description": "a starter for fastify project using mongoose",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --forceExit",
    "test:coverage": "jest --coverage --runInBand --forceExit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "babel-node index.js",
    "start:watch:prod": "ENV=production nodemon --exec babel-node index.js",
    "start:watch:dev": "ENV=development nodemon --exec babel-node index.js"
  },
  "keywords": [
    "rest api",
    "fastify",
    "mongodb"
  ],
  "author": "izzy",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fastify": "^3.14.0",
    "fastify-auth": "^1.0.1",
    "fastify-print-routes": "^0.4.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "joi": "^17.4.0",
    "joi-password-complexity": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "pino": "^6.11.2",
    "pino-pretty": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "^25.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "mockdate": "^2.0.5",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/config/index.js"
    ]
  }
}
